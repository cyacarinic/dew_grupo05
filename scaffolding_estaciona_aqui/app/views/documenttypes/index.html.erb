<p id="notice"><%= notice %></p>

<h1>Documenttypes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documenttypes.each do |documenttype| %>
      <tr>
        <td><%= documenttype.nombre %></td>
        <td><%= link_to 'Show', documenttype %></td>
        <td><%= link_to 'Edit', edit_documenttype_path(documenttype) %></td>
        <td><%= link_to 'Destroy', documenttype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documenttype', new_documenttype_path %>
