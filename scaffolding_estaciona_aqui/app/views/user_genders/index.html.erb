<p id="notice"><%= notice %></p>

<h1>User Genders</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_genders.each do |user_gender| %>
      <tr>
        <td><%= user_gender.nombre %></td>
        <td><%= link_to 'Show', user_gender %></td>
        <td><%= link_to 'Edit', edit_user_gender_path(user_gender) %></td>
        <td><%= link_to 'Destroy', user_gender, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Gender', new_user_gender_path %>
