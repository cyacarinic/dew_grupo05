<p id="notice"><%= notice %></p>

<h1>Additionalservices</h1>

<table>
  <thead>
    <tr>
      <th>Parking</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Precio hora</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @additionalservices.each do |additionalservice| %>
      <tr>
        <td><%= additionalservice.parking_id %></td>
        <td><%= additionalservice.nombre %></td>
        <td><%= additionalservice.descripcion %></td>
        <td><%= additionalservice.precio_hora %></td>
        <td><%= link_to 'Show', additionalservice %></td>
        <td><%= link_to 'Edit', edit_additionalservice_path(additionalservice) %></td>
        <td><%= link_to 'Destroy', additionalservice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Additionalservice', new_additionalservice_path %>
