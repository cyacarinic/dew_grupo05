<%= form_with(model: parking, local: true) do |form| %>
  <% if parking.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(parking.errors.count, "error") %> prohibited this parking from being saved:</h2>

      <ul>
      <% parking.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id, "Usuario" %>
    <!-- <%= form.number_field :user_id, id: :parking_user_id %> -->
    <%= form.collection_select :user_id, User.all, :id, :correo %>
  </div>

  <div class="field", "Tipo">
    <%= form.label :parkingtype_id %>
    <!-- <%= form.number_field :parkingtype_id, id: :parking_parkingtype_id %> -->
    <%= form.collection_select :parkingtype_id, Parkingtype.all, :id, :nombre %>
  </div>

  <div class="field">
    <%= form.label :nombre %>
    <%= form.text_field :nombre, id: :parking_nombre %>
  </div>

  <div class="field">
    <%= form.label :descripcion %>
    <%= form.text_field :descripcion, id: :parking_descripcion %>
  </div>

  <div class="field">
    <%= form.label :telefono %>
    <%= form.text_field :telefono, id: :parking_telefono %>
  </div>

  <div class="field">
    <%= form.label :precio_hora %>
    <%= form.text_field :precio_hora, id: :parking_precio_hora %>
  </div>

  <div class="field">
    <%= form.label :imagen %>
    <%= form.text_field :imagen, id: :parking_imagen %>
  </div>

  <div class="field">
    <%= form.label :departamento %>
    <%= form.text_field :departamento, id: :parking_departamento %>
  </div>

  <div class="field">
    <%= form.label :ciudad %>
    <%= form.text_field :ciudad, id: :parking_ciudad %>
  </div>

  <div class="field">
    <%= form.label :distrito %>
    <%= form.text_field :distrito, id: :parking_distrito %>
  </div>

  <div class="field">
    <%= form.label :direccion %>
    <%= form.text_field :direccion, id: :parking_direccion %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
