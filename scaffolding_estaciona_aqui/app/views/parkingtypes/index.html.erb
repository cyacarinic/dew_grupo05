<p id="notice"><%= notice %></p>

<h1>Parkingtypes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkingtypes.each do |parkingtype| %>
      <tr>
        <td><%= parkingtype.nombre %></td>
        <td><%= link_to 'Show', parkingtype %></td>
        <td><%= link_to 'Edit', edit_parkingtype_path(parkingtype) %></td>
        <td><%= link_to 'Destroy', parkingtype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parkingtype', new_parkingtype_path %>
