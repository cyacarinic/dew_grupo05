<p id="notice"><%= notice %></p>

<h1>Parking Histories</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Parking</th>
      <th>Fecha inicio</th>
      <th>Fecha fin</th>
      <th>Fecha registro</th>
      <th>Fecha cancelacion</th>
      <th>Motivo cancelacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_histories.each do |parking_history| %>
      <tr>
        <td><%= parking_history.user_id %></td>
        <td><%= parking_history.parking_id %></td>
        <td><%= parking_history.fecha_inicio %></td>
        <td><%= parking_history.fecha_fin %></td>
        <td><%= parking_history.fecha_registro %></td>
        <td><%= parking_history.fecha_cancelacion %></td>
        <td><%= parking_history.motivo_cancelacion %></td>
        <td><%= link_to 'Show', parking_history %></td>
        <td><%= link_to 'Edit', edit_parking_history_path(parking_history) %></td>
        <td><%= link_to 'Destroy', parking_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking History', new_parking_history_path %>
